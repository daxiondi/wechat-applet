<!--pages/register/register.wxml-->
<form class="page__bd" bindsubmit="formSubmit">
 <view class="section1">
    <input name="phone" placeholder="请输入手机号"/>
  </view>
   <view class="section2">
    <input name="nickname" placeholder="请输入昵称"/>
  </view>
